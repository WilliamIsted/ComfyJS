{
  "name": "comfy.js",
  "version": "2.0.0",
  "description": "Comfiest Twitch Integration Module",
  "main": "dist/comfy.js",
  "type": "commonjs",
  "module": "dist/comfy.mjs",
  "exports": {
    ".": {
      "import": "./dist/comfy.mjs",
      "require": "./dist/comfy.js"
    }
  },
  "scripts": {
    "example": "ts-node-dev --respawn --transpile-only src/example.ts",
    "dev": "vite --open",
    "watch": "vite build --watch",
    "build": "npm-run-all --sequential build:*",
    "build:normal": "vite build",
    "build:minified": "vite build -c vite.config.min.js",
    "build:version": "node version.js",
    "test": "jest",
    "start": "npm run build && node build/index.js",
    "clean": "rimraf ./dist/",
    "lint": "eslint . --ext .js,.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/instafluff/ComfyJS.git"
  },
  "author": "Instafluff",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/instafluff/ComfyJS/issues"
  },
  "homepage": "https://github.com/instafluff/ComfyJS#readme",
  "devDependencies": {
    "@types/jest": "^29.2.5",
    "@types/node": "^18.14.2",
    "@typescript-eslint/eslint-plugin": "^5.48.0",
    "@typescript-eslint/parser": "^5.48.0",
    "dotenv": "^8.2.0",
    "eslint": "^8.31.0",
    "eslint-config-comfycase": "^0.1.2",
    "jest": "^29.3.1",
    "npm-run-all": "^4.1.5",
    "rimraf": "^3.0.2",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^4.9.4",
    "vite": "^4.1.4"
  },
  "dependencies": {
    "ws": "^8.12.1"
  }
}
